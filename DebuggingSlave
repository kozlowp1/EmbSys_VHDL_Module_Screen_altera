force -freeze sim:/masterslaveinterface/Clk 1 0, 0 {50 ps} -r 100
force -freeze sim:/masterslaveinterface/as_nReset 0 0 -cancel 100

force -freeze sim:/masterslaveinterface/as_Address 010 300 -cancel 400 
force -freeze sim:/masterslaveinterface/as_Write 1 300  -cancel 400 
force -freeze sim:/masterslaveinterface/as_ChipSelect 1 300  -cancel 400 
force -freeze sim:/masterslaveinterface/as_WriteData 11110000111100001111000011110000 300  -cancel 400 

